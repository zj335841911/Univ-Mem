<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.ui.NewsContentActivity">


    <LinearLayout
        android:id="@+id/newsContentActivity_back1"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@color/color_f43530">

        <ImageView
            android:id="@+id/newsContentActivity_back"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/univ_mem_icon_back" />

    </LinearLayout>

    <WebView
        android:id="@+id/newsContentActivity_webView"
        android:layout_width="match_parent"
        android:layout_height="689dp"
        android:layout_below="@+id/newsContentActivity_back1"
        android:layout_alignParentBottom="true"
        android:layout_marginTop="0dp"
        android:layout_marginBottom="0dp" />
</RelativeLayout>
